<config xsi:noNamespaceSchemaLocation="SDKSchema.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" >
    <!--缓存队列设置-->
    <offlineCache>
        <!--建议 length * timeout <=iteration -->
        <!--queue长度，>=lengthValue立刻提交-->
        <length>0</length>
        <!-- 每次读取存储url的Sharedpreferenced的间隔时间 -->
        <queueExpirationSecs>300</queueExpirationSecs>
        <!--发送超时时间-->
        <timeout>60</timeout>
    </offlineCache>
    
    <companies>
