                    </argument>
                    <argument>
                        <key>MUDS</key>
                        <value>0i</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>
                    <argument>
                        <key>REDIRECTURL</key>
                        <value>u</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>
