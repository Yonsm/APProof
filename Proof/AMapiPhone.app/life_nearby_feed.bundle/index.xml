        <!-- 附近服务 style6====================================== -->
        <cell height="wrap" id="general_big_word" intype="layout" orientation="vertical"
            width="fill" bgcolor="#FFFFFF">
            
            
            <!-- 内容区域-->
            <panel height="wrap" margin="30,24,0,4" width="fill">
                <!-- 标题视图-->
                <label font="32" height="wrap" id="mainName"
