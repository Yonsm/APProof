<ui title="附近">
    <cells bgcolor="#FFFFFF" id="feedcells">
        <!--附近团购 style1=========================== -->
        
        <cell height="wrap" id="tag" intype="layout" orientation="vertical"
            width="fill" bgcolor="#FFFFFF">
            
            
            <!-- 内容区域-->
            <panel height="wrap" margin="30,24,0,4" orientation="horizontal" width="fill">
                <layout height="wrap"
                    padding="0,0,0,20" orientation="vertical" panel_align="left_top"
                    percent_width="67">
                    <!-- 标题视图-->
                    <label align="left" font="32" height="wrap" id="mainName"
                    line="1" textcolor="#333333" width="fill"
                    />
                    <!-- 内容视图-->
                    <label align="left" font="24" height="wrap" id="textRecommend"
                    line="3" margin="20,0,0,6" textcolor="#999999"
                    width="fill"
                    />
                </layout>
                <image height="w*0.67"
                id="mainPic"
                panel_align="right_top"
                percent_width="33"
                default_image="nearby_feed_pics_default_pic.png"
                padding="0,0,0,20"
                />
            </panel>
            <!--底部视图-->
            <panel
                height="wrap"
                margin="20,24,0,24"
                width="fill">
                <image height="26" id="iconLeft"
                panel_align="left_center"
                width="26"
                />
                <!-- 底部右边标签-->
                <label font="22"
                height="wrap"
                id="textRight"
                panel_align="right_center" textcolor="#999999"
                width="wrap"
                />
                <label font="22" height="wrap"
                id="textLeft" leftof="textRight"
                margin="2,14,0,0" rightof="iconLeft"
                textcolor="#999999" width="fill"/>
                
            </panel>
            <!-- 线-->
            <layout align="left" bgcolor="#E6E6E6" height="1"
            margin="30,24,0,24"
            width="fill"/>
        </cell>
        <!-- 酒店样式 style2================================ -->
        <cell height="wrap" id="general_pics" intype="layout"
            orientation="vertical" width="fill" bgcolor="#FFFFFF">
            <!--标题-->
            <label align="left" font="32" height="wrap" id="mainName"
            margin="30,24,0,24"
            textcolor="#333333"
            width="fill"
            />
            <!-- 图片区域-->
            <layout height="wrap" margin="20,24,0,4" orientation="horizontal" width="fill">
                <image height="w*0.65" id="leftPic" padding="0,0,0,20"
                percent_width="34"
                default_image="nearby_feed_pics_default_pic.png"
                />
                <image height="w*0.67" id="midPic" padding="0,0,0,20"
                percent_width="33"
                default_image="nearby_feed_pics_default_pic.png"
                />
                <image height="w*0.67" id="rightPic" padding="0,0,0,20"
                percent_width="33"
                default_image="nearby_feed_pics_default_pic.png"
                />
            </layout>
            
            <!--底部视图-->
            <panel
                height="wrap"
                margin="20,24,0,24"
                width="fill">
                <image height="26" id="iconLeft"
                panel_align="left_center"
                width="26"
                />
                <!-- 底部右边标签-->
                <label font="22" height="wrap"
                id="textRight"
                panel_align="right_center"
                textcolor="#999999"
                width="wrap"
                />
                <label font="22" height="wrap"
                id="textLeft" leftof="textRight"
                margin="2,14,0,0"
                rightof="iconLeft"
                textcolor="#999999"
                width="fill"/>
            </panel>
            <!-- 线-->
            <layout align="left" bgcolor="#E6E6E6" height="1"
            margin="30,24,0,24"
            width="fill"/>
            
        </cell>
        <!-- 周边游样式 style3================================ -->
        <cell height="wrap" id="general_big_pic" intype="layout" orientation="vertical"
            width="fill" bgcolor="#FFFFFF">
            
            <!-- 标题视图-->
            <label align="left" font="32" height="wrap" id="mainName"
            margin="30,24,0,24" textcolor="#333333" width="fill"
            />
            <!-- 图片区域-->
            <panel height="w*0.29" id="mainPic" margin="20,24,0,24" width="fill"  default_bgimage="nearby_feed_big_pic_default_pic.png">
                <!-- 底部阴影视图-->
                <image height="60" image="bg.png" panel_align="center_bottom"
                width="fill"
                />
                <!-- 阴影视图上层容器-->
                <panel height="40" panel_align="center_bottom" width="fill">
                    <!-- 为了避免IOS中无法同时应用 panel_align rightof 之类的属性 加一个视图居左-->
                    <layout height="1" hidden="true" id="nearby_travel_view"
                        panel_align="left_bottom"
                        width="1">
                    </layout>
                    <label font="24" height="fill" id="picRight"
                    padding="0,0,10,20"
                    panel_align="right_bottom" textcolor="#FFFFFF"
                    width="wrap"
                    />
                    <label font="24" height="wrap" id="picLeft"
                    leftof="picRight" padding="0,20,10,10"
                    rightof="nearby_travel_view"
                    textcolor="#FFFFFF"
                    width="fill"
                    />
                </panel>
            </panel>
            <!--底部视图-->
            <panel
                height="wrap"
                margin="20,24,0,24"
                width="fill">
                <image height="26" id="iconLeft"
                panel_align="left_center"
                width="26"
                />
                <!-- 底部右边标签-->
                <label font="22"
                height="wrap"
                id="textRight"
                panel_align="right_center"
                textcolor="#999999"
                width="wrap"
                />
                <label font="22" height="wrap"
                id="textLeft" leftof="textRight"
                margin="2,14,0,0" rightof="iconLeft"
                textcolor="#999999"
                width="fill"/>
            </panel>
            
            <!-- 线-->
            <layout align="left" bgcolor="#E6E6E6" height="1"
            margin="30,24,0,24"
            width="fill"/>
        </cell>
