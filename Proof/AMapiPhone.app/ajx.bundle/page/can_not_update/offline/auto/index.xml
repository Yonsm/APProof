<ui barColor="#5192ff">
    <styles>
        <style id="rootLayoutStyle" bgcolor="#AAAAAAFF" width="fill" height="fill" />
        <style id="top_panel_style" width="fill" height="96"/>
        <style id="center_panel_style" width="fill" height="fill" bgcolor="#FFFFFF"/>
        <style id="title_label_style" textColor="#FFFFFF" align="center" height="wrap" width="wrap" font="36"/>
        <style id="header-left" margin="4, 12, 4, 12" panel_align="left_center" width="100" height="88"/>
        <style id="title_icon_style" panel_align="right_center" width="98" height="78"/>
        <style id="image_wrap" width="wrap" height="wrap" scalemode="aspectfill"/>

        <style id="top_panel_style_container" width="fill" height="356" bgcolor="#5192ff" orientation="vertical"/>
        <style id="top_panel_car_state_lable" width="wrap" height="wrap" textColor="#9ec9ef" font="22"/>
        <style id="load_more" text="下载更多城市数据" stretch="15,15,15,15" align="center" textColor="#333333" width="260" height="56" clickable="true">
          <state id="normal" bgimage="./image/button_white_normal.png"/>
          <state id="highlighted" bgimage="./image/button_white_highlighted.png"/>
        </style>
        <style id="send_all_style" text="全部发送" width="fill" font="32" height="90" stretch="14,14,14,14" align="center" margin="20,20,20,20" clickable="true">
          <state id="normal" bgimage="./image/button_send_all_normal.png" textColor="#FFFFFF" />
          <state id="highlighted" bgimage="./image/button_send_all_highlighted.png" textColor="#FFFFFF" />
          <state id="disabled" bgimage="./image/button_send_all_diabled.png" textColor="#FFFFFF80"/>
        </style>
        <style id="city_data_panel_style" width="fill" height="fill" bgcolor="#FFFFFF"/>

        <!--发送页面start-->
        <!-->title样式<-->
        <style id="sending_title_style" text="数据发送中" textcolor="FFFFFF" align="center" font="34"></style>
        <style id="success_title_style" text="发送完成" textcolor="FFFFFF" align="center" font="34"></style>
        <style id="failed_title_style" text="发送失败" textcolor="FFFFFF" align="center" font="34"></style>
        <style id="disconnected_title_style" text="Wi-Fi连接已断开" textcolor="FFFFFF" align="center" font="34"></style>

        <!-->状态图标样式<-->
        <style id="sending_icon_style" bgimage="./image/send_loading.png" width="240" height="240" panel_align="center"></style>
        <style id="success_full_icon_style" bgimage="./image/send_success_full.png" width="240" height="240" panel_align="center"></style>
        <style id="success_part_icon_style" bgimage="./image/send_success_part.png" width="240" height="240" panel_align="center"></style>
        <style id="failed_icon_style" bgimage="./image/send_failed.png" width="240" height="240" panel_align="center"></style>
        <style id="disconnected_icon_style" bgimage="./image/disconnected.png" width="240" height="240" panel_align="center"></style>

        <!-->状态描述样式<-->
        <style id="sending_desc_style" text="数据发送中请勿做其他操作\n保持手机靠近汽车，防止连接中断" textcolor="FFFFFFBA" font="28" line="2" align="center"></style>
        <style id="success_full_desc_style" text="成功发送x/x个城市" textcolor="FFFFFFBA" font="28" margin="60,0,0,0" align="center"></style>
        <style id="success_part_desc_style" text="成功发送1/x个城市" textcolor="FFFFFFBA" font="28" margin="60,0,0,0" align="center"></style>
        <style id="failed_desc_style" text="请重新发送" textcolor="FFFFFFBA" font="28" margin="60,0,0,0" align="center">></style>
        <style id="disconnected_desc_style" text="请检查手机热点状态" textcolor="FFFFFFBA" font="28" margin="60,0,0,0" align="center">></style>

        <!-->确定按钮样式<-->
        <style id="ok_btn_style" text="确定" font="30" height="88" width="400" align="center" stretch="14,14,14,14">
          <state id="normal" bgimage="./image/ok_button_normal.png" textcolor="FFFFFF"></state>
          <state id="highlighted" bgimage="./image/ok_button_highlighted.png" textcolor="FFFFFFBA"></state>
        </style>

        <!-->停止发送按钮样式<-->
        <style id="stop_btn_style" text="停止发送" font="30" height="88" width="400" align="center" stretch="14,14,14,14">
            <state id="normal" bgimage="./image/stop_send_btn_normal.png" textcolor="FFFFFF"></state>
            <state id="highlighted" bgimage="./image/stop_send_btn_highlighted.png" textcolor="FFFFFFBA"></state>
        </style>

        <style id="overly" bgcolor="#00000048" hidden="true" width="fill" height="fill"></style>
        <style id="overly_content" panel_align="center_bottom" bgcolor="#fafafa" height="wrap" width="fill"></style>
        <style id="overly_btn_item" height="90" border_width="1" border_color="#E2E2E2" textcolor="#0091FF" font="34" align="center" clickable="true">
          <state id="normal" bgcolor="FFFFFF"></state>
          <state id="highlighted" bgcolor="#E0E0E0"></state>
        </style>
        <style id="overly_btn_cancel" textcolor="#333333" height="90" border_width="1" border_color="#E2E2E2" font="34" align="center" clickable="true">
          <state id="normal" bgcolor="FFFFFF"></state>
          <state id="highlighted" bgcolor="#E0E0E0"></state>
        </style>

        <style id="overly_title" height="wrap" textcolor="#666666" align="center" font="26" ></style>
        <style id="overly_sub_title" height="wrap" textcolor="#FF5B5A" align="center" font="20" margin="12,0,0,0"></style>

        <!--发送页面end-->

        <style id="blue_button_style" textcolor="#0091FF" font="24" stretch="15,15,15,15" align="center">
          <state id="normal" bgimage="./image/button_blue_normal.png"/>
          <state id="highlighted" bgimage="./image/button_blue_highlighted.png"/>
        </style>
        <style id="list_divider" height="1" width="fill" bgcolor="#E2E2E2"/>
        <style id="cell_province_item_style" height="121" width="fill" orientation="vertical" bgcolor="#FFFFFF"></style>
        <style id="cell_province_content_style" height="120" width="fill" padding="0,24,0,24"></style>
        <style id="cell_province_province_name_style" width="wrap" height="fill" font="32" textcolor="#333333" panel_align="left_center" align="left_center"></style>
        <style id="cell_province_province_update_status_style" width="wrap" height="fill" font="26" textcolor="#0091FF" align_bottom="province_name" rightof="province_name" margin="0,12,0,0" align="left_center"></style>
        <style id="cell_province_province_group_indicator_style" width="wrap" height="wrap" panel_align="right_center"></style>

        <style id="cell_city_item_style" height="121" width="fill" orientation="vertical" align="left_center" bgcolor="#F4F4F4"></style>
        <style id="cell_city_content_style" height="120" width="fill" padding="0,24,0,24" bgcolor="#F4F4F4"></style>
        <style id="cell_city_name_style" width="wrap" height="wrap" font="32" textcolor="#333333" panel_align="left_center" align="left_center"></style>
        <style id="cell_city_current_city_style" text="(当前城市)" width="wrap" height="wrap" font="26" textcolor="#999999" rightof="city_name" align_bottom="city_name" align="left_center"></style>
        <style id="cell_city_update_status_style" width="wrap" height="wrap" font="26" textcolor="#0091FF" rightof="current_city" align_bottom="current_city" margin="0,12,0,0" align="left_center"></style>
        <style id="cell_city_send_button_style" width="80" height="44" panel_align="right_center" stretch="15,15,15,15" align="center" textcolor="#0091FF" font="24" text="发送" clickable="true">
           <state id="normal" bgimage="./image/button_blue_normal.png"/>
           <state id="highlighted" bgimage="./image/button_blue_highlighted.png"/>
        </style>
        <style id="cell_city_continue_send_button_style" width="120" height="44" panel_align="right_center" stretch="15,15,15,15" align="center" textcolor="#FF0000" font="24" text="继续发送" clickable="true">
           <state id="normal" bgimage="./image/button_red_normal.png"/>
           <state id="highlighted" bgimage="./image/button_red_highlighted.png"/>
        </style>
        <style id="cell_city_data_status_style" font="26" textcolor="#999999" width="wrap" height="wrap" panel_align="right_center"/>
        <style id="cell_city_data_size_style" text="234.6M" width="wrap" height="wrap" font="26" textcolor="#999999" margin="20,0,0,0"/>
    </styles>

    <container>
        <panel style="rootLayoutStyle">
            <panel id="listPage">
                <layout id="top_panel_container" width="fill" height="wrap" bgcolor="#3D93FD" orientation="vertical" panel_align="center_top">
                    <!--title栏-->
                    <panel id="titleBarPanel" style="top_panel_style" panel_align="center_top">
                        <panel action="aui://page?act=backward" style="header-left" id="hl" clickable="true">
                            <image image="./image/titleicon_back_baise.png" height="64" width="64" panel_align="left_center"/>
                        </panel>
                        <label id="title" style="title_label_style" text="发送车机版地图数据" panel_align="center"/>
                        <image id="connect_state" image="./image/link.png" rightof="title" margin="0,20,0,0"
                               panel_align="center" style="image_wrap" height="32" width="34"/>
                    </panel>
                    <layout id="carStatePanel" width="fill" height="268" orientation="vertical" align="center_top">
                        <image id="car_state_img" image="./image/car.png"  width="234" height="124" scalemode="aspectfill" margin="30,0,0,0"/>
                        <label style="top_panel_car_state_lable" text="车机剩余空间" margin="15,0,0,0"/>
                        <layout id="storage_size_container" width="wrap" height="wrap" orientation="horizontal" align="center_top" margin="10,0,0,0">
                            <label id="availble_size_text" style="top_panel_car_state_lable" text="--"/>
                            <label style="top_panel_car_state_lable" text="/"/>
                            <label id="total_size_text" style="top_panel_car_state_lable" text="--"/>
                        </layout>
                    </layout>
                </layout>

                <panel id="bottomPanel" style="city_data_panel_style" panel_align="center" below="top_panel_container" align="center">
                    <layout id="cityDataHeaderPanel" width="fill" height="wrap" orientation="vertical" panel_align="center_top">
                        <label width="fill" height="60" text="可发送至汽车的数据" padding="0,24,0,24" font="26" textcolor="#666666" bgcolor="#FFFFFF" align="left_center"/>
                        <panel id="updateInfoPanel" width="fill" height="60" bgcolor="#FFFBC9F5" padding="0,24,0,9" align="left_center" hidden="true">
                            <image id="warn_icon" width="wrap" height="wrap" image="./image/warn.png" scalemode="aspectfill" panel_align="left_center" margin="0,0,0,12"/>
                            <image id="close" width="60" height="60" padding="15,15,15,15" image="./image/close.png" scalemode="aspectfill" panel_align="right_center"/>
                            <label width="fill" height="fill" text="部分数据有更新，建议先更新后再发送到汽车" textcolor="#976c22" font="24" align="left_center" leftof="close" rightof="warn_icon" />
                        </panel>
                    </layout>
                    <panel id="cityDataPanel" width="fill" height="fill" panel_align="center" below="cityDataHeaderPanel" hidden="true">
                        <layout id="sendAllLayout" width="fill" height="wrap" orientation="vertical" align="center" panel_align="center_bottom">
                            <label style="list_divider"/>
                            <label id="sendAll" style="send_all_style"/>
                        </layout>
                        <list id="city_list" width="fill" height="fill" above="sendAllLayout"/>
                    </panel>
                    <layout id="emptyView" width="fill" height="fill" orientation="vertical" panel_align="center" align="center" hidden="true">
                        <image image="./image/empty.png" width="wrap" height="wrap" scalemode="aspectfill"/>
                        <label width="wrap" height="wrap" font="28" textColor="#999999" margin="28,0,60,0" text="无可发送的数据"/>
                        <label id="loadMore" style="load_more" font="28" />
                    </layout>
                </panel>
            </panel>

            <panel style="overly" id="overly" clickable="true">
              <panel style="overly_content" id="overly_content">
                  <layout id="title_bar_container" orientation="vertical" height="wrap" width="fill" bgcolor="#FAFAFA" panel_align="center_top" >
                    <layout orientation="vertical" height="90" width="fill" bgcolor="#FAFAFA" align="center">
                      <label text="北京"  style="overly_title" id="overly_title"></label>
                      <label text="地图+导航都下载后，才可以发送给汽车"  style="overly_sub_title" id="overly_sub_title"></label>
                    </layout>
                    <label text="去下载完整数据" style="overly_btn_item" id="overly_item_0"></label>
                    <label text="去下载完整数据" style="overly_btn_item" id="overly_item_1"></label>
                      <label text="去下载完整数据" style="overly_btn_item" id="overly_item_2"></label>
                    <label text="取消" style="overly_btn_cancel" id="overly_item_cancel" margin="20,0,0,0"></label>
                  </layout>
              </panel>
            </panel>

            <!--发送页面start -->
            <panel id="sendPage" bgcolor="#000000D9" padding="-40,0,0,0" hidden="true">
                <layout height="wrap" panel_align="center" align="center">
                    <label id="titleLabel" style="sending_title_style"></label>

                    <!-->发送中的状态图标单独布局<-->
                    <panel id="progressPanel" height="wrap" margin="60,0,0,0" hidden="false">
                        <image id="animateStateIcon" style="sending_icon_style"></image>
                        <layout width="wrap" height="wrap" panel_align="center" align="center">
                            <panel width="wrap">
                                <label id="progressLabel" text="0" width="wrap" textcolor="#31c464" font="60" translatey="8"></label>
                                <label id="percentLabel" text="%" width="wrap" textcolor="#31c464" font="28" rightof="progressLabel" align_bottom="progressLabel"></label>
                            </panel>
                            <label id="progressDescLabel" text="已发送1/x个城市" font="22" textcolor="FFFFFFBA" width="wrap" margin="7,0,0,0"></label>
                        </layout>
                    </panel>

                    <!-->状态图标<-->
                    <image id="stateIcon" style="success_full_icon_style" margin="60,0,0,0" hidden="true"></image>

                    <panel width="fill" height="140" margin="60,0,0,0">
                        <label id="stateDescLabel" style="sending_desc_style" height="wrap" margin="0,80,0,80" panel_align="center_top"></label>
                        <!-->部分发送失败label<-->
                        <label id="failedCityDescLabel" text="xx市、xxx市、xx市等x个城市发送失败" height="wrap" font="28" textcolor="FB8D35" line="2" align="center"
                               below="stateDescLabel" margin="20,80,0,80" hidden="true"></label>
                    </panel>

                    <label id="operateBtn" style="stop_btn_style"></label>
                </layout>
            </panel>
            <!--发送页面end-->
        </panel>
    </container>
</ui>
